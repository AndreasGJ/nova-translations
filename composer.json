{
    "name": "agj/nova-translations",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "spatie/laravel-translatable": "^3.1.3"
    },
    "autoload": {
        "psr-4": {
            "Agj\\NovaTranslations\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Agj\\NovaTranslations\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
